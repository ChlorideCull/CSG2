server {
	listen 8080;
	listen [::]:8080;
	server_name %%SERVERNAME%%;
    error_page 502 /_/err/badgateway.html;
    error_page 504 /_/err/timeout.html;
    location /_/err/ {
        internal;
        root @POSTINSTALLPATH@/share/csg2/nginx-default/;
    }
	location / {
		proxy_pass http://unix:%%SOCKETPATH%%:;
	}
}
